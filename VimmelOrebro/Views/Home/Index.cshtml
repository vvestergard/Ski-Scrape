@{
    ViewBag.Title = "Ski Scrape";
}
<head>
    <title></title>
    <script src="~/Scripts/lightbox.js"></script>
    <script src="~/Scripts/jquery-2.2.3.js"></script>
    <link href="~/Content/lightbox.css" rel="stylesheet" />

    <script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/jquery/1.4/jquery.min.js"></script>
    <script type="text/javascript" src="/fancybox/jquery.fancybox-1.3.4.pack.js"></script>
    <link rel="stylesheet" href="/fancybox/jquery.fancybox-1.3.4.css" type="text/css" media="screen" />

</head>
<body>

    <div class="wrapper">
       


        @for (int i = 0; i < ViewBag.antal; i++)
        {

            <div class="col span_1_of_2 imgCont centerImg">
                @*<a href="@ViewBag.link[i]" rel="lightbox" data-lightbox="image-1">
                        <img src="@ViewBag.link[i]" />
                    </a>*@
                <a id="singleImg" href="@ViewBag.link[i]"><img src="@ViewBag.link[i]" alt="" /></a>
                <script>
    $(document).ready(function () {

        /* This is basic - uses default settings */

        $("a#singleImg").fancybox();

        /* Using custom settings */

        $("a#inline").fancybox({
            'hideOnContentClick': true
        });

        /* Apply fancybox to multiple items */

        $("a.group").fancybox({
            'transitionIn': 'elastic',
            'transitionOut': 'elastic',
            'speedIn': 600,
            'speedOut': 200,
            'overlayShow': false
        });

    });
                </script>

            </div>
        }
    </div>







</body>